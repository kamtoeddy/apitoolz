{
  "author": "kamtoeddy",
  "description": "javascript server-side helper tools",
  "dependencies": {
    "callsite": "^1.0.0",
    "formidable": "^2.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/formidable": "^2.0.5",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.0",
    "express": "^4.17.3",
    "jest": "^28.1.3",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "typescript": "^4.6.2"
  },
  "main": "dist/index.js",
  "name": "apitoolz",
  "files": [
    "dist/**/*"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/(*.)+(test).(ts|js)"
    ],
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ],
    "setupFilesAfterEnv": [
      "./src/__tests__/app/setEnvVars.js"
    ]
  },
  "license": "ISC",
  "scripts": {
    "build": "tsc",
    "clean-up": "rm -rf coverage dist",
    "test": "tsc & jest --verbose --watchAll",
    "test-c": "tsc & jest --verbose --watchAll --coverage",
    "test:ci": "tsc & jest --verbose",
    "prepare": "npm run clean-up && npm run build"
  },
  "types": "dist/index.d.ts",
  "version": "0.2.2"
}
